<h1>Listing server_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @server_categories.each do |server_category| %>
  <tr>
    <td><%= server_category.name %></td>
    <td><%= link_to 'Show', server_category %></td>
    <td><%= link_to 'Edit', edit_server_category_path(server_category) %></td>
    <td><%= link_to 'Destroy', server_category, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

